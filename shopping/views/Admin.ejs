<style>
.usersList td{
    text-align:center;
    height:30px;
    vertical-align:middle;
    border-left:1px solid #dbdbdb;
    border-top:1px solid #dbdbdb;}
}

</style>              
<% posts.forEach(function(post){ %>
				<tr style="background:#f4f4f4;">
					<td style="font-weight:bold;"><%= post.name%></td>
					<td><%= post.phoneNumber%></td>
					<td><%= post.email%></td>
					<td><%= post.Address%></td>
					<td><%= post.flag%></td>
                    <td>null</td>
                    <td id="delect" method ="POST" type="button" value="Delect" width="90%" class="btn btn-danger">Delect</td>
