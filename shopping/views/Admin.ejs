<% include ../include/header.ejs %>
<%include ../include/header_usermanage.ejs%>
<style>
.usersList-table{position:relative;}
.usersList-table table{color:#777;border-right:1px solid #dbdbdb;border-bottom:1px solid #dbdbdb;background:url(images/TI.jpg) top left no-repeat;margin-bottom:15px;}
.usersList .list{height:44px;line-height:44px;color:#fff;font-size:16px;font-weight:bold;font-family:'微软雅黑','幼圆';width:740px;}


