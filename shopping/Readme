<script type="text/javascript">

  $(document).ready(function () {
       $("#search").keyup(function(event){
         $.ajax({
             url: "/searchitems",
             type: "POST",
             data: 'search=' + $("#search").val(),
             success: function (msg) {
              //   var datas = eval('(' + msg.name + ')');
             // var cc=JSON.stringify(msg);
                  $('#infoBox').slideDown(300);
                if(msg == ''){
                    $("#infoBox").html('');
                    var str = '没有匹配的数据';
                    $("#infoBox").append("<p>"+str+"</p>");
                }
                else{
                    $("#infoBox").html('');
                    var len = msg.length;
                  
                    for(var i = 0; i <len; i++){
                        $("#infoBox").append("<p>"+msg[i]+"</p>");
                    }
                }
             }
         });
     });
  });
</script>



